package java_basics;

public class ControlFlow {

	public static void main(String[] args) {
		boolean hungry = true;
		
		if(hungry) {
			System.out.println("I'm starving...");
		}else if(!hungry){
			System.out.println("I'm not hungry");
		}else {
			System.out.println("You can either be hungry or not hungry nothing else");
		}
		
		int hungerRating = 5;
		if(hungerRating < 6) {
			System.out.println("Not hungry!");
		} else {
			System.out.println("I'm starving...");
		}
	}
}
